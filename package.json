{
  "name": "tssrp6a",
  "version": "3.0.0",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.ts"
  ],
  "types": "dist/index.d.ts",
  "private": false,
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "big-integer": "^1.6.48"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@types/tape-promise": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "microbundle": "^0.13.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.2.0",
    "source-map-support": "^0.5.19",
    "tap-diff": "^0.1.1",
    "tap-nyc": "^1.0.3",
    "tape": "^5.2.2",
    "tape-promise": "^4.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build": "yarn clean && microbundle -f cjs,esm src/index.ts",
    "clean": "rm -rf dist",
    "format": "prettier '**/*.{js,md,ts,tsx}'",
    "lint": "yarn format --check && yarn eslint src test",
    "test": "tape 'test/**/*.test.ts' -r ts-node/register",
    "test:coverage": "nyc -x 'src/cross-env-crypto.ts' -x 'test/**' yarn test | tap-nyc",
    "tscheck": "tsc --noEmit"
  },
  "description": "SRP6a client and server lib",
  "repository": "git@github.com:midokura/platform-frontend.git",
  "author": "Midokura",
  "license": "Apache-2.0",
  "keywords": [
    "srp",
    "srp6a",
    "authentication"
  ],
  "homepage": "https://github.com/midonet/tssrp6a"
}
